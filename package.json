    "build": "vite build",
    "build:turbo": "node scripts/turbo-build.js",
    "build:full": "node scripts/fast-full-build.js",
    "build:full-safe": "npm run type-check:incremental && npm run build:fast",
    "build:check": "npm run type-check && vite build",
    "build:parallel": "concurrently \"npm run type-check\" \"vite build\" --kill-others-on-fail",
    "build:fast": "vite build",
    "build:ultra": "node scripts/ultra-build.js",
    "build:ultra-fast": "echo '‚ö° ÂêØÂä®Ë∂ÖÂø´ÁªàÊûÅÊûÑÂª∫...' && npm run type-check:fast && npm run lint:fast && npm run build:fast && echo '‚úÖ Ë∂ÖÂø´ÁªàÊûÅÊûÑÂª∫ÂÆåÊàê'",
    "type-check": "node scripts/type-check.js",
    "type-check:fast": "tsc --noEmit --skipLibCheck --incremental",
    "type-check:incremental": "tsc --noEmit --incremental",
    "type-check:tsc": "tsc --noEmit",
    "type-check:strict": "echo 'üîç ÊâßË°å‰∏•Ê†ºÁ±ªÂûãÊ£ÄÊü•...' && tsc --noEmit --strict --exactOptionalPropertyTypes && echo '‚úÖ ‰∏•Ê†ºÁ±ªÂûãÊ£ÄÊü•ÈÄöËøá'",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint .",
    "lint:fast": "eslint . --cache --cache-location=node_modules/.tmp/eslint-cache",
    "check:full": "node scripts/fast-full-check.js",
    "preview": "vite preview",
    "build:android": "npm run build && npx cap sync android",
    "buildandroid": "npm run build && npx cap sync android",
    "open:android": "npx cap open android",
    "build:apk": "npm run build:android && npm run open:android",
    "mobile:capacitor": "npm run build:android",
    "mobile:tauri": "npm run build:tauri-android",
    "mobile:both": "npm run mobile:capacitor && npm run mobile:tauri",
    "buildapk": "npm run buildandroid && npm run open:android",
    "dev:android": "npm run dev -- --host && npx cap run android",
    "devandroid": "npm run dev -- --host && npx cap run android",
    "build:ios": "npm run build && npx cap sync ios",
    "buildios": "npm run build && npx cap sync ios",
    "buildandios": "npm run build && npx cap sync android && npx cap sync ios",
    "open:ios": "npx cap open ios",
    "dev:ios": "npm run dev -- --host && npx cap run ios",
    "devios": "npm run dev -- --host && npx cap run ios",
    "clean": "rimraf dist",
    "rebuild": "npm run clean && npm run build",
    "docker:build": "docker build -t aetherlink .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "hf:prepare": "chmod +x huggingface/prepare.sh && ./huggingface/prepare.sh",
    "hf:build": "cd huggingface && docker build -t aetherlink-hf .",
    "hf:test": "docker run -p 7860:7860 aetherlink-hf",
    "install:npm": "npm install",
    "install:yarn": "yarn install",
    "devyarn": "yarn dev",
    "buildyarn": "yarn build",
    "tauri": "tauri",
    "dev:tauri": "tauri dev",
    "build:tauri": "tauri build",
    "dev:tauri-ios": "tauri ios dev",
    "build:tauri-ios": "tauri ios build",
    "dev:tauri-android": "tauri android dev",
    "build:tauri-android": "tauri android build",
    "build:tauri-android:release": "tauri android build",
    "build:tauri-android:debug": "tauri android build --debug",
    "build:tauri-android:arm64": "tauri android build --target aarch64",
    "build:tauri-android:split": "tauri android build --split-per-abi",
