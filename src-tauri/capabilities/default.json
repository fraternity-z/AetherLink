{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://**" },
        { "url": "http://**" }
      ]
    },
    {
      "identifier": "advanced-file-manager:default",
      "allow": [
        { "path": "**" }
      ]
    },
    "shell:default",
    "shell:allow-open"
  ]
}
